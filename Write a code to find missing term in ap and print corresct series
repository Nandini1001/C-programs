#include<stdio.h>
#include<stdlib.h>
void missing(int[],int);
void main()
{
    int n,*p,i;
    scanf("%d",&n);
    p=(int*)malloc((n+1)*sizeof(int));
    if(p==NULL)
        printf("Memory not allocated");
    else
    {
        for(i=0;i<n;i++)
            scanf("%d",(p+i));
        missing(p,n);
    }
}
void missing(int a[],int n)
{
    int i,d,j;
    if((a[1]-a[0])==(a[n-1]-a[n-2]))
        d=a[1]-a[0];
    else if((a[1]-a[0])==a[2]-a[1])
            d=a[1]-a[0];
    else
            d=a[n-1]-a[n-2];
        for(i=0;i<n-1;i++)
            {
                if((a[i+1]-a[i])!=d)
                {
                    if((a[i]+2*d)==a[i+1])
                    {
                        for(j=n;j>=i+1;j--)
                            a[j]=a[j-1];
                        a[i+1]=a[i]+d;
                        break;
                    }
                }
            }
            for(i=0;i<n+1;i++)
                printf("%d ",a[i]);
}
