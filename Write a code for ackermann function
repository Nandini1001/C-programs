#include<stdio.h>
int ack(int,int);
void main()
{
    int m,n,ans;
    printf("Enter 2 values: ");
    scanf("%d%d",&n,&m);
    ans=ack(m,n);
    printf("%d",ans);
}
int ack(int m,int n)
{
    if(m==0)
        return n+1;
    else if(m>0 && n==0)
        ack(m-1,1);
    else
        ack(m-1,ack(m,n-1));
}
