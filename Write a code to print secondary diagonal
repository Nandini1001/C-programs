#include<stdio.h>
void main()
{
    int n,m,r,t;
    scanf("%d %d",&n,&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
            scanf("%d",&a[i][j]);
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {

        if(i+j==m-1)
        {
            printf("%d",a[i][j]);
        }
        else
        printf(" ");
        }
        printf("\n");
    }

}
