#include <stdio.h>
void main() {
    int n,m,j;
    scanf("%d%d",&n,&m);
    int a[n][m],i;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
        printf("Enter a[%d][%d]",i,j);
        scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++)
    {

        for(j=0;j<m;j++)
        {
            printf("%d ",a[i][j]);
        }
         printf("\n");
    }

}
