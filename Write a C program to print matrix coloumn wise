#include <stdio.h>
void main()
{
    int n,m,j;
    printf("Enter rows and coloumns");
    scanf("%d%d",&n,&m);
    int a[n][m],i,s=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
           printf("Enter a[%d][%d]",i,j);
           scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d ",a[j][i]);
        }
    }
}
//OUTPUT
Enter rows and coloumns3
3
Enter a[0][0]2
Enter a[0][1]9
Enter a[0][2]5
Enter a[1][0]8
Enter a[1][1]4
Enter a[1][2]6
Enter a[2][0]2
Enter a[2][1]2
Enter a[2][2]8
2 8 2 9 4 2 5 6 8
