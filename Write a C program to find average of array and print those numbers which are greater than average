#include<stdio.h>
void main()
{
    int n;
    printf("Enter range of array");
    scanf("%d",&n);
    int a[n],i,j,avg,s=0;
    for(i=0;i<n;i++)
    {
        printf("a[%d]=",i);
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        s=s+a[i];
    }
    avg=(float)s/n;
    for(i=0;i<n;i++)
    {
        if(a[i]>avg)
            printf("%d\t",a[i]);
    }
}
//OUTPUT
Enter range of array5
a[0]=2
a[1]=8
a[2]=6
a[3]=5
a[4]=8
8       6       8
