#include <stdio.h>
void main() 
{
    int n,m,j;
    scanf("%d%d",&n,&m);
    int a[n][m],i,s=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
        printf("Enter a[%d][%d]",i,j);
        scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        s=0;
        for(j=0;j<n;j++)
        {
            s=s+a[j][i];

        }
         printf("sum of %d coloumn is %d",i+1,s);
    }
}
