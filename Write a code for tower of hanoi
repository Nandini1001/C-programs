#include<stdio.h>
void toh(int,char,char,char);
void main()
{
    char src='A',des='C',help='B';
    int n;
    printf("Enter no of discs");
    scanf("%d",&n);
    toh(n,src,help,des);
}
void toh(int n,char src,char help, char des)
{
    if(n==1)
    {
        printf("%c->%c ",src,des);
        return;
    }
    toh(n-1,src,des,help);
    printf("%c->%c ",src,des);
    toh(n-1,help,src,des);
}
